  "tokenColors": [
    {
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded"
      ],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "name": "Comment", // 注释
      "scope": "comment",
      "settings": {
        // "foreground": "#88846f"
        "foreground": "#6b6b6b"
      }
    },
    {
      "name": "String", // 字符串
      "scope": "string",
      "settings": {
        // "foreground": "#E6DB74"
        "foreground": "#efb946"
      }
    },
    {
      "name": "Template Definition", // 模板定义
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        // "foreground": "#F92672"
        "foreground": "#67a23c"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression", // 重置JavaScript字符串插值表达式
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "name": "Number", // 数字
      "scope": "constant.numeric", // 常数数字的
      "settings": {
        "foreground": "#778dff"
      }
    },
    {
      "name": "Built-in constant", // 内置常量
      "scope": "constant.language",
      "settings": {
        "foreground": "#ea00ff"
      }
    },
    {
      "name": "User-defined constant", // 用户定义常数
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#AE81FF"
        // "foreground": "#ff004e"
      }
    },
    {
      "name": "Variable", // 变量名称
      "scope": "variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#fdfdfd"
      }
    },
    {
      "name": "Keyword", // 关键字
      "scope": "keyword",
      "settings": {
        "foreground": "#ff004e"
      }
    },
    {
      "name": "Storage", // 存储
      "scope": "storage",
      "settings": {
        "foreground": "#ff004e"
      }
    },
    {
      "name": "Storage type", // 存储类型 var let const
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic", // 斜体
        "foreground": "#f993f3"
      }
    },
    {
      "name": "Class name", // 类名
      "scope": "entity.name.type, entity.name.class, entity.name.namespace, entity.name.scope-resolution",
      "settings": {
        "fontStyle": "underline", // 下划线
        "foreground": "#7cd131"
      }
    },
    {
      "name": "Inherited class", // 继承的类名
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#7cd131"
      }
    },
    {
      "name": "Function name", // 函数名
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#7cd131"
      }
    },
    {
      "name": "Function argument", // 函数参数
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#fd6d1f"
      }
    },
    {
      "name": "Tag name", // 标签名
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#61a9ff"
      }
    },
    {
      "name": "Tag attribute", // 标签的属性名
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#2ee2a5"
      }
    },
    {
      "name": "Library function", // 函数库
      "scope": "support.function",
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Library constant", // 库常数
      "scope": "support.constant",
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Library class/type", // 图书馆类别/类型
      "scope": "support.type, support.class",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Invalid", // 无效的
      "scope": "invalid",
      "settings": {
        "fontStyle": "",
        "foreground": "#F44747"
      }
    },
    {
      "name": "Invalid deprecated", // 无效的不推荐
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "name": "JSON String", // json 字符串
      "scope": "meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "#d3e2dd"
      }
    },
    {
      "name": "diff.header",
      "scope": "meta.diff, meta.diff.header",
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "name": "diff.deleted", // 差异删除
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#AE81FFA0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "name": "Markup Styling",
      "scope": "markup.bold, markup.italic",
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#FD971F"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#A6E22E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    // Markdown
    {
      "name": "Markdown Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#75715E"
      }
    },
    {
      "name": "Markdown Bold", // Markdown 引用
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link Title/Description",
      "scope": "string.other.link.title.markdown,string.other.link.description.markdown",
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Markdown Underline Link/Image",
      "scope": "markup.underline.link.markdown,markup.underline.link.image.markdown",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "name": "Markdown Emphasis",
      "scope": "markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Punctuation Definition Link",
      "scope": "markup.list.unnumbered.markdown, markup.list.numbered.markdown",
      "settings": {
        "foreground": "#f8f8f2"
      }
    },
    {
      "name": "Markdown List Punctuation",
      "scope": [
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#FD971F"
      }
    }
  ],
  "semanticHighlighting": true
}
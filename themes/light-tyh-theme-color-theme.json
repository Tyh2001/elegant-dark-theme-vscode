{
  "name": "tyh-theme-light",
  "type": "light",
  "colors": {
    "editor.background": "#ffffff", // 编辑器背景色
    // 左侧活动栏
    "activityBar.background": "#2ba245", // 活动栏背景色。活动栏显示在最左侧或最右侧，并允许在侧边栏的视图间切换。
    "activityBar.foreground": "#f1f1f1",
    "activityBarBadge.background": "#f1f1f1",
    "titleBar.activeBackground": "#ffffff",
    "tab.activeBackground": "#ffffff", // 选中的背景色
    "tab.inactiveBackground": "#eeeeee", // 未选中的选项卡
    "tab.activeForeground": "#2c2c2c", // 关闭按钮颜色
    "tab.unfocusedHoverBackground": "#ffffff", // 鼠标移入后的背景色
    "statusBar.noFolderBackground": "#6690fa",
    "statusBar.debuggingBackground": "#6690fa",
    "peekView.border": "#6690fa",
    "statusBar.background": "#6690fa",
    "button.background": "#6690fa",
    "badge.background": "#6690faAA",
    "progressBar.background": "#6690fa",
    // 
    "focusBorder": "#A6B39B",
    "pickerGroup.foreground": "#A6B39B",
    "pickerGroup.border": "#749351",
    "list.activeSelectionForeground": "#6c6c6c",
    "quickInputList.focusBackground": "#CADEB9",
    "list.hoverBackground": "#e0e0e0",
    "list.activeSelectionBackground": "#c4d9b1",
    "list.inactiveSelectionBackground": "#d3dbcd",
    "list.highlightForeground": "#9769dc",
    "selection.background": "#C9D0D9",
    // "editor.background": "#F5F5F5",
    "editorWhitespace.foreground": "#AAAAAA",
    "editor.lineHighlightBackground": "#E4F6D4",
    "editorLineNumber.activeForeground": "#9769dc",
    "editor.selectionBackground": "#C9D0D9",
    "minimap.selectionHighlight": "#C9D0D9",
    "panel.background": "#F5F5F5",
    "sideBar.background": "#F2F2F2",
    "sideBarSectionHeader.background": "#ede8ef",
    "editorLineNumber.foreground": "#6D705B",
    "editorCursor.foreground": "#54494B",
    "inputOption.activeBorder": "#adafb7",
    "dropdown.background": "#F5F5F5",
    "editor.findMatchBackground": "#BF9CAC",
    "editor.findMatchHighlightBackground": "#edc9d8",
    "peekViewEditor.matchHighlightBackground": "#C2DFE3",
    "peekViewTitle.background": "#F2F8FC",
    "peekViewEditor.background": "#F2F8FC",
    "peekViewResult.background": "#F2F8FC",
    "peekViewResult.matchHighlightBackground": "#93C6D6",
    "tab.lastPinnedBorder": "#c9d0d9",
    "welcomePage.tileBackground": "#f0f0f7",
    "statusBarItem.remoteBackground": "#4e3c69",
    "ports.iconRunningProcessForeground": "#749351",
    // 下面两个是侧边栏的颜色和hover配置
    "editorGroup.dropBackground": "#C9D0D988",
    "inputValidation.infoBorder": "#4ec1e5",
    "inputValidation.infoBackground": "#f2fcff",
    "inputValidation.warningBackground": "#fffee2",
    "inputValidation.warningBorder": "#ffe055",
    "inputValidation.errorBackground": "#ffeaea",
    "inputValidation.errorBorder": "#f1897f",
    "errorForeground": "#f1897f",
    "walkThrough.embeddedEditorBackground": "#00000014",
    "editorIndentGuide.background": "#aaaaaa60",
    "editorIndentGuide.activeBackground": "#777777b0"
  },
  "tokenColors": [
    {
      "name": "全局设置",
      "settings": {
        "foreground": "#333333"
      }
    },
    // HTML部分
    {
      "name": "HTML 标签的尖括号",
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#a8a8a8"
      }
    },
    {
      "name": "HTML 标签名",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#0063dd"
      }
    },
    {
      "name": "HTML 标签属性名",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#05d18d"
      }
    },
    // js部分
    {
      "name": "JavaScript 和 HTML 字符串",
      "scope": "string",
      "settings": {
        "foreground": "#d49204"
      }
    },
    {
      "name": "JavaScript 和 HTML 标点符号",
      "scope": "punctuation",
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "name": "JavaScript 布尔值",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#00d3b7"
      }
    },
    {
      "name": "JavaScript 数字",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#5885ff"
      }
    },
    {
      "name": "JavaScript 变量名",
      "scope": "variable",
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "name": "JavaScript 方法的名 querySelector",
      "scope": "entity.name, entity.name.type.class, support.type, support.class, meta.use",
      "settings": {
        "foreground": "#0703eb"
      }
    },
    {
      "name": "JavaScript class类的方法",
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#62d300"
      }
    },
    {
      "name": "JavaScript 和 HTML 注释",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#adb5b6"
      }
    },
    {
      "name": "JavaScript 关键字 const function",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f001f8"
      }
    },
    {
      "name": "JavaScript 数字",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#778dff"
      }
    },
    {
      "name": "JavaScript 变量名",
      "scope": "variable",
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "name": "JavaScript 类名",
      "scope": "entity.name.type, entity.name.class, entity.name.namespace, entity.name.scope-resolution",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#7cd131"
      }
    },
    {
      "name": "JavaScript 继承的类名",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#7cd131"
      }
    },
    {
      "name": "JavaScript 函数参数",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00cece"
      }
    },
    {
      "name": "JavaScript 关键字 await return",
      "scope": "keyword",
      "settings": {
        "foreground": "#e70448"
      }
    },
    {
      "name": "JavaScript 关键字 async",
      "scope": "storage",
      "settings": {
        "foreground": "#e70448"
      }
    },
    {
      "name": "JavaScript 模板字符串中的 ${}",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#e70448"
      }
    },
    {
      "name": "JavaScript this 关键字",
      "scope": "variable.language",
      "settings": {
        "foreground": "#06c5c5"
      }
    },
    {
      "name": "JavaScript 函数名",
      "scope": "meta.function entity.name.function",
      "settings": {
        "foreground": "#61cc04"
      }
    },
    // JSON
    {
      "name": "JSON 一级的属性名",
      "scope": [
        "meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#f8844e"
      }
    },
    {
      "name": "JSON 二级的属性名",
      "scope": [
        "meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#e79b0e"
      }
    },
    {
      "name": "JSON 字符串",
      "scope": "meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "#038b5e"
      }
    },
    // CSS
    {
      "name": "CSS class 选择器",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#7cd131"
      }
    },
    {
      "name": "CSS ID 选择器",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#ff7010"
      }
    },
    {
      "name": "CSS 标签选择器",
      "scope": "source.css entity.name.tag",
      "settings": {
        "foreground": "#61a9ff"
      }
    },
    {
      "name": "CSS 属性名",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#7cd131"
      }
    },
    // Markdown
    {
      "name": "Markdown 链接 - []",
      "scope": "string.other.link.title.markdown,string.other.link.description.markdown",
      "settings": {
        "foreground": "#4d79da"
      }
    },
    {
      "name": "Markdown 引用 - >",
      "scope": "markup.quote.markdown",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c68fce"
      }
    },
    {
      "name": "Markdown 重点 - ****",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#b551f0"
      }
    },
    {
      "name": "Markdown 地址 - ()",
      "scope": "markup.underline.link.markdown,markup.underline.link.image.markdown",
      "settings": {
        "foreground": "#b0f1db"
      }
    },
    {
      "name": "Markup 标题",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Markup 标题符号 #",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#A6E22E",
        "fontStyle": "bold"
      }
    },
  ],
}
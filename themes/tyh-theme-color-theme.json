  "tokenColors": [
    // 注释
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#bdc6cd"
      }
    },
    // 关键字 const function
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic", // 斜体
        "foreground": "#f993f3"
      }
    },
    // 关键字 return
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#ff004e"
      }
    },
    // JSON 字符串
    {
      "name": "JSON String", // json 字符串
      "scope": "meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "#b0f1db"
      }
    },
    // 标签名
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#61a9ff"
      }
    },
    // 标签的属性名
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#2ee2a5"
      }
    },
    // 字符串
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#efb946"
      }
    },
    // 函数名
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#7cd131"
      }
    },
    // 数字
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#778dff"
      }
    },
    // 变量名
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#ffffff"
      }
    },
    // 类名
    {
      "name": "Class name",
      "scope": "entity.name.type, entity.name.class, entity.name.namespace, entity.name.scope-resolution",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#7cd131"
      }
    },
    // 继承的类名
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#7cd131"
      }
    },
    // 函数参数
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#70d32e"
      }
    },
    // Markdown
    // 链接 - []
    {
      "name": "Markdown Link Title/Description",
      "scope": "string.other.link.title.markdown,string.other.link.description.markdown",
      "settings": {
        "foreground": "#4d79da"
      }
    },
    // 引用 - >
    {
      "name": "Markdown Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c68fce"
      }
    },
    // 重点 - ****
    {
      "name": "Markdown Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#b551f0"
      }
    },
    // 地址 - ()
    {
      "name": "Markdown Underline Link/Image",
      "scope": "markup.underline.link.markdown,markup.underline.link.image.markdown",
      "settings": {
        "foreground": "#b0f1db"
      }
    },
